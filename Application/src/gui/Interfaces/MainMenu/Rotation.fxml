<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.layout.Pane?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Label?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.Interfaces.MainMenu.RotationController"
            prefHeight="400.0" prefWidth="600.0" stylesheets="/gui/style.css">
    <center>
        <HBox styleClass="hbox" spacing="20" prefWidth="Infinity">
            <BorderPane styleClass="=borderPane">
                <center>
                    <Pane>
                        <ImageView>
                            <Image url="@../../images/logo.PNG"></Image>
                        </ImageView>
                        <Label fx:id="imageLabel"></Label>
                    </Pane>
                </center>
                <bottom>
                    <ScrollBar fx:id="scrollBar"  HBox.hgrow="ALWAYS"  ></ScrollBar>
                </bottom>
            </BorderPane>
        </HBox>
    </center>
</BorderPane>