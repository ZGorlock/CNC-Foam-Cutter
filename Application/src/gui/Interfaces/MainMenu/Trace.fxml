<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.SwingNode?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Tab text="Tracer" closable="false" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Interfaces.MainMenu.TraceController">
    <BorderPane styleClass="borderPane">
        <center>
            <AnchorPane id="traceAnchorPane" prefHeight="640.0" prefWidth="1080.0" styleClass="pane">
                <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <center>
                        <SwingNode fx:id="swingNodeTrace"/> <!-- TODO get this to be centered in the anchor even if the screen changes dimensions (fullscreen) -->
                    </center>
                </BorderPane>
            </AnchorPane>
        </center>
        <bottom>
            <HBox spacing="20" alignment="CENTER" styleClass="hbox">
                <Label fx:id="grblX"/>
                <Label fx:id="grblY"/>
                <Label fx:id="grblZ"/>
                <Label fx:id="grblA"/>
            </HBox>
        </bottom>
    </BorderPane>
</Tab>
