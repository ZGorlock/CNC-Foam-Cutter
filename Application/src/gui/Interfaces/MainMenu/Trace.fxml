<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.SwingNode?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<Tab text="Tracer" closable="false" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Interfaces.MainMenu.TraceController">
    <BorderPane styleClass="borderPane">
        <center>
            <Pane id="paneTrace" prefHeight="600.0" prefWidth="600.0" stylesheets="/gui/style.css" styleClass="pane">
                <SwingNode fx:id="swingNodeTrace"/>
            </Pane>
        </center>
        <bottom>
            <HBox spacing="20" alignment="CENTER" styleClass="hbox">
                <Label fx:id="grblX"/>
                <Label fx:id="grblY"/>
                <Label fx:id="grblZ"/>
                <Label fx:id="grblA"/>
            </HBox>
        </bottom>
    </BorderPane>
</Tab>
