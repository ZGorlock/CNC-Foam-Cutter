<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="200.0" prefWidth="300.0" stylesheets="/gui/style.css" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.interfaces.popup.JobCompletedController">

    <HBox id="header" centerShape="false" prefHeight="63.0" prefWidth="600.0" stylesheets="/gui/style.css"
          xmlns="http://javafx.com/javafx/8.0.121">
        <ImageView id="logo" preserveRatio="true" fitHeight="100">
            <Image url="@../../images/logo.PNG"/>
        </ImageView>
        <Label id="header" alignment="CENTER" contentDisplay="RIGHT" maxWidth="Infinity" prefHeight="100.0"
               prefWidth="537.0" styleClass="title" text="3D CNC FOAM CUTTER" textAlignment="CENTER" HBox.hgrow="ALWAYS"
               VBox.vgrow="ALWAYS">
        </Label>
        <ImageView id="logo2" preserveRatio="true" fitHeight="100">
            <Image url="@../../images/logo.PNG"/>
        </ImageView>
    </HBox>

    <BorderPane prefHeight="100.0" prefWidth="300.0" styleClass="borderPane">
        <bottom>
            <VBox alignment="CENTER" spacing="20">
                <Label alignment="TOP_RIGHT" contentDisplay="CENTER" text="Job Completed!" textAlignment="CENTER"
                       VBox.vgrow="ALWAYS"/>
                <ImageView id="knightPopUp" fitHeight="200" fitWidth="300.0" preserveRatio="true" VBox.vgrow="ALWAYS">
                    <Image url="@../../images/knight.PNG"/>
                </ImageView>
                <Label fx:id="timeCompleted" text="00:00:00" VBox.vgrow="ALWAYS"/>
                <Button id="ok" alignment="CENTER" onAction="#ok" styleClass="buttonGold" text="Charge On!"
                        VBox.vgrow="ALWAYS"/>
            </VBox>
        </bottom>
    </BorderPane>
</VBox>
